    "# Utiliser la fiche technique pour créer un dictionnaire avec les paramètres des lignes\n",
    "\n",
    "# Ajouter une colonne \"length_km\" à net_trey.line avec les longueurs des lignes en km\n",
    "net_trey.line.insert(\n",
    "    1, \"length_km\", [0.180, 0.1, 0.115, 0.08, 0.09, 0.135, 0.1, 0.205, 0.085, 0.255, 0.340]\n",
    ")\n",
    "\n",
    "## Créer une nouvelle colonne \"cable_type\" en supprimant la valeur indésirable (_x) utilisée pour différencier chaque ligne à partir de \"name\" avec une regex\n",
    "# re.sub remplace les correspondances par la chaîne de remplacement donnée\n",
    "# Premier argument : motif regex\n",
    "# Deuxième argument : chaîne de remplacement\n",
    "# Troisième argument : chaîne d'entrée\n",
    "net_trey.line[\"cable_type\"] = net_trey.line.apply(\n",
    "    lambda x: re.sub(r\"_[0-9]$\", \"\", x[\"name\"]), axis=1\n",
    ")\n",
    " \n",
    "## Créer une table de paramètres pour les types de câbles\n",
    "cable_types = pd.DataFrame(\n",
    "    {\n",
    "        \"cable_type\": [\"GKN3x150_150\", \"GKN3X95_95\", \"GKN3X50_50\", \"GKN3X240_240\", \"GKT3X50_50\"],\n",
    "        \"r_ohm_per_km\": [0.124, 0.193, 0.387, 0.0754, 0.387],\n",
    "        \"x_ohm_per_km\": [0.07, 0.07, 0.07, 0.07, 0.07],\n",
    "        \"c_uf_per_km\": [349, 338, 298, 346, 298],\n",
    "        \"max_i_ka\": [0.400, 0.252, 0.170, 0.512, 0.170], # les valeurs de max_i_ka sont les valeurs pour des câbles enterrés. En effet le réseau suisse est majoritairement souterrain en zone urbaine. \n",
    "    }\n",
    ")\n",
    " \n",
    "## Fusionner net_trey.line avec cable_types sur \"cable_type\"\n",
    "net_trey.line = net_trey.line.merge(cable_types, on=\"cable_type\", how=\"left\")\n",
    " \n",
    "##\n",
    " \n",
    "## Montre la mise à jour de net_trey.line\n",
    "net_trey.line"
